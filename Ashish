def second_largest_distinct_element(arr):
    # 
    unique_arr = list(set(arr))
    # 
    unique_arr.sort(reverse=True)
    # 
    if len(unique_arr) < 2:
        return -1
    return unique_arr[1]
arr = [2, 3, 5, 6, 6, 5, 7, 8, 9, 9, 10]
print(second_largest_distinct_element(arr)) 
                 or 
def second_largest_distinct_element(arr):
    unique_arr = list(set(arr))
    unique_arr.sort(reverse=True)
    if len(unique_arr) < 2:
        return -1
    return unique_arr[1]
arr = list(map(int, input("Enter elements separated by space: ").split()))

print(second_largest_distinct_element(arr))
